# Production environment template (NO SECRETS)
ENVIRONMENT=production

# PostgreSQL (Cloud SQL recommended)
POSTGRES_HOST=cloudsql-hostname-or-proxy
POSTGRES_PORT=5432
POSTGRES_USER=superuser
POSTGRES_PASSWORD=replace_me
POSTGRES_AIRFLOW_DB=airflow
POSTGRES_MLFLOW_DB=mlflow

# Redis (Memorystore recommended)
REDIS_HOST=memorystore-host
REDIS_PORT=6379
# REDIS_PASSWORD=replace_me_if_needed

# Storage (GCS)
STORAGE_GCS_BUCKET=webook-mlops-artifacts

# MLflow
MLFLOW_TRACKING_URI=http://mlflow.your-domain-or-cluster:5000
MLFLOW_ARTIFACT_ROOT=gs://webook-mlops-artifacts/mlflow

# Airflow
AIRFLOW_WEBSERVER_URL=http://airflow.your-domain-or-cluster:8080

# Feast offline store (BigQuery)
FEAST_OFFLINE_STORE=bigquery
FEAST_BIGQUERY_DATASET=webook_mlops_feast

